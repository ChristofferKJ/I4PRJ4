<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:forms="clr-namespace:Lottie.Forms;assembly=Lottie.Forms"
             x:Class="App1.OmOsPage"
             BackgroundColor="White">
    <ContentPage.Content>
        <Grid Padding="50">

            <forms:AnimationView

                x:Name="animationView" 
                Animation="process_complete.json" 
                Loop="True" 
                AutoPlay="True"/>


            <Label TextColor="Black" Text="ASE StudyHelper er en applikation udviklet i forbindelse med et semesterprojekt på uddannelsen Information- og Kommunikationsteknologi."/>

        </Grid>
    </ContentPage.Content>
</ContentPage>